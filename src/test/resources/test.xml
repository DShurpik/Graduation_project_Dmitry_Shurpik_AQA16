<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd">


<suite name="test" thread-count="2" parallel="tests">

    <test name="Create delete user" enabled="true">
        <classes>
            <class name="CreateDeleteUser"/>
            <parameter name="name" value="John"/>
            <parameter name="lastName" value="Doe"/>
            <parameter name="email" value="pasasdsasasiiiiijet476@nubotel.com"/>
            <parameter name="password" value="d12345678"/>
        </classes>
    </test>

    <test name="Add medicine on favorite" enabled="true">
        <classes>
            <class name="AddMedicineOnFavorite"/>
            <parameter name="medicine1" value="Кардиомагнум"/>
            <parameter name="medicine2" value="Аспирин кардио"/>
        </classes>
    </test>

    <test name="Check Pop up and Dialog window" enabled="true">
        <classes>
            <class name="CheckPopUpMsg"/>
            <class name="DialogWindow"/>
            <parameter name="medicine" value="Аспирин кардио"/>
        </classes>
    </test>

    <test name="Incorrect data use" enabled="true">
        <classes>
            <class name="IncorrectDataUse"/>
            <parameter name="name" value="John"/>
            <parameter name="lastName" value="Doe"/>
            <parameter name="email" value="asasasddassddasd@te.vom"/>
            <parameter name="password" value="!!!!!!!!"/>
        </classes>
    </test>

    <test name="Limit values use" enabled="true">
        <classes>
            <class name="LimitValuesUse"/>
            <parameter name="name" value="John"/>
            <parameter name="lastName" value="Doe"/>
            <parameter name="email" value="testaqa1@dtest.com"/>
            <parameter name="password" value="d123456"/>
        </classes>
    </test>

    <test name="test with bug" enabled="true">
        <classes>
            <class name="RepeatBug"/>
            <parameter name="name" value="John"/>
            <parameter name="lastName" value="Doe"/>
            <parameter name="email1" value="testingaqa116@test.vom"/>
            <parameter name="email2" value="testingaqa1161@test.vom"/>
            <parameter name="password1" value="aaaaaaaa"/>
            <parameter name="password2" value="12345678"/>
        </classes>
    </test>

    <test name="Upload file" enabled="true">
        <classes>
            <class name="UploadFile"/>
        </classes>
    </test>

</suite>